<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="450.0" prefWidth="800.0" stylesheets="@AplicationEntry.css" vgap="15.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestiune.farmacie.controllers.CreateCategController">
   <children>
      <Label prefHeight="30.0" prefWidth="166.0" text="Adauga Categorie ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="fieldId" disable="true" editable="false" prefHeight="26.0" prefWidth="166.0" promptText="id" />
      <TextField fx:id="fieldTitlu" prefHeight="26.0" prefWidth="166.0" promptText="Titlu" />
      <TextArea fx:id="areaDescriere" prefHeight="137.0" prefWidth="263.0" promptText="Descriere">
         <font>
            <Font size="13.0" />
         </font>
      </TextArea>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" prefHeight="30.0" prefWidth="200.0">
         <children>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#addCategory" text="Adauga" />
         </children>
      </FlowPane>
   </children>
</FlowPane>
